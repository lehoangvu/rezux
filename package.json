  "name": "tiki-mobile-web",
  "version": "1.0.0",
  "description": "React, Redux, React Router, Universal",
  "main": "server/index.js",
  "scripts": {
    "dev": "webpack -d --watch --color  --progress --config webpack.babel.js",
    "start": "run-s build start-server-production",
    "build": "run-p build-server build-client",
    "watch": "run-p watch-client",
    "watch-client": "webpack -d --watch --color --config webpack.babel.js",
    "watch-server": "webpack -d --watch --color --config webpack.server.babel.js",
    "build-client": "webpack --color --config webpack.client.production.babel.js",
    "build-server": "webpack --color --config webpack.server.production.babel.js",
    "start-server-development": "supervisor --no-restart-on error -w server/index.js server",
    "start-server-production": "node server"
  },
  "repository": {
    "type": "git",
